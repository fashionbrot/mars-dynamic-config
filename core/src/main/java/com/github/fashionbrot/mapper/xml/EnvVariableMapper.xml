<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.github.fashionbrot.mapper.EnvVariableMapper">


    <resultMap type="com.github.fashionbrot.entity.EnvVariableEntity" id="envVariableMap">
            <result property="id" column="id"/>
            <result property="variableName" column="variable_name"/>
            <result property="variableDesc" column="variable_desc"/>
            <result property="variableKey" column="variable_key"/>
            <result property="createId" column="create_id"/>
            <result property="createDate" column="create_date"/>
            <result property="delFlag" column="del_flag"/>
    </resultMap>

    <resultMap type="com.github.fashionbrot.dto.EnvVariableDTO" id="envVariableDtoMap">
            <result property="id" column="id"/>
            <result property="variableName" column="variable_name"/>
            <result property="variableDesc" column="variable_desc"/>
            <result property="variableKey" column="variable_key"/>
            <result property="createId" column="create_id"/>
            <result property="createDate" column="create_date"/>
            <result property="delFlag" column="del_flag"/>
    </resultMap>
    <sql id="allSql">
        id,variable_name,variable_desc,variable_key,create_id,create_date,del_flag
    </sql>

    

</mapper>
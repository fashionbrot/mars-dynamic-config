<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.github.fashionbrot.mapper.SysRoleMapper">


    <resultMap type="com.github.fashionbrot.entity.SysRoleEntity" id="sysRoleMap">
            <result property="id" column="id"/>
            <result property="roleName" column="role_name"/>
            <result property="createDate" column="create_date"/>
            <result property="updateDate" column="update_date"/>
            <result property="delFlag" column="del_flag"/>
    </resultMap>

    <resultMap type="com.github.fashionbrot.dto.SysRoleDTO" id="sysRoleDtoMap">
            <result property="id" column="id"/>
            <result property="roleName" column="role_name"/>
            <result property="createDate" column="create_date"/>
            <result property="updateDate" column="update_date"/>
            <result property="delFlag" column="del_flag"/>
    </resultMap>
    <sql id="allSql">
        id,role_name,create_date,update_date,del_flag
    </sql>

    

</mapper>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.github.fashionbrot.mapper.EnvMapper">


    <resultMap type="com.github.fashionbrot.entity.EnvEntity" id="envMap">
            <result property="id" column="id"/>
            <result property="envCode" column="env_code"/>
            <result property="envDesc" column="env_desc"/>
            <result property="createId" column="create_id"/>
            <result property="createDate" column="create_date"/>
            <result property="delFlag" column="del_flag"/>
    </resultMap>

    <resultMap type="com.github.fashionbrot.dto.EnvDTO" id="envDtoMap">
            <result property="id" column="id"/>
            <result property="envCode" column="env_code"/>
            <result property="envDesc" column="env_desc"/>
            <result property="createId" column="create_id"/>
            <result property="createDate" column="create_date"/>
            <result property="delFlag" column="del_flag"/>
    </resultMap>
    <sql id="allSql">
        id,env_code,env_desc,create_id,create_date,del_flag
    </sql>

    

</mapper>
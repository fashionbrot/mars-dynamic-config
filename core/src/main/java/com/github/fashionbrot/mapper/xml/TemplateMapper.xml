<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.github.fashionbrot.mapper.TemplateMapper">


    <resultMap type="com.github.fashionbrot.entity.TemplateEntity" id="templateMap">
            <result property="id" column="id"/>
            <result property="appCode" column="app_code"/>
            <result property="templateKey" column="template_key"/>
            <result property="templateName" column="template_name"/>
            <result property="templateDesc" column="template_desc"/>
            <result property="createId" column="create_id"/>
            <result property="createDate" column="create_date"/>
            <result property="delFlag" column="del_flag"/>
    </resultMap>

    <resultMap type="com.github.fashionbrot.dto.TemplateDTO" id="templateDtoMap">
            <result property="id" column="id"/>
            <result property="appCode" column="app_code"/>
            <result property="templateKey" column="template_key"/>
            <result property="templateName" column="template_name"/>
            <result property="templateDesc" column="template_desc"/>
            <result property="createId" column="create_id"/>
            <result property="createDate" column="create_date"/>
            <result property="delFlag" column="del_flag"/>
    </resultMap>
    <sql id="allSql">
        id,app_code,template_key,template_name,template_desc,create_id,create_date,del_flag
    </sql>

    

</mapper>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.github.fashionbrot.mapper.DynamicDataValueMapper">


    <resultMap type="com.github.fashionbrot.entity.DynamicDataValueEntity" id="dynamicDataValueMap">
            <result property="id" column="id"/>
            <result property="dataId" column="data_id"/>
            <result property="json" column="json"/>
            <result property="tempJson" column="temp_json"/>
            <result property="delFlag" column="del_flag"/>
    </resultMap>

    <resultMap type="com.github.fashionbrot.dto.DynamicDataValueDTO" id="dynamicDataValueDtoMap">
            <result property="id" column="id"/>
            <result property="dataId" column="data_id"/>
            <result property="json" column="json"/>
            <result property="tempJson" column="temp_json"/>
            <result property="delFlag" column="del_flag"/>
    </resultMap>
    <sql id="allSql">
        id,data_id,json,temp_json,del_flag
    </sql>

    

</mapper>